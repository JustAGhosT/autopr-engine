[flake8]
max-line-length = 100
extend-ignore = E203, W503
per-file-ignores = 
    __init__.py:F401
exclude = 
    .venv,
    .git,
    .mypy_cache,
    .pytest_cache,
    __pycache__,
    .coverage,
    htmlcov,
    build,
    dist,
    .eggs,
    *.egg-info,
    migrations,
    docs/_build,
    node_modules,
    site-packages
